ni-area style=\"height: 45px\" />\n      </a-col>\n      <a-col style=\"padding: 0 34px; margin-bottom: 24px\" :sm=\"12\" :xs=\"24\">\n        <div class=\"num-info\">\n          <span class=\"title\">\n            {{$t('capita')}}\n            <a-tooltip :title=\"$t('introduce')\">\n              <a-icon type=\"info-circle\" style=\"font-size: 14px; margin-left: 8px\" />\n            </a-tooltip>\n          </span>\n          <div class=\"value\">\n            <span class=\"total\">2.7</span>\n            <span class=\"subtotal\">71.2<a-icon type=\"caret-down\" /></span>\n          </div>\n        </div>\n        <mini-area style=\"height: 45px\" />\n      </a-col>\n    </a-row>\n    <a-table\n      :dataSource=\"searchData\"\n      :columns=\"tableColumns\"\n      :pagination=\"{style: { marginBottom: 0 }, pageSize: 5}\"\n      size=\"small\"\n      rowKey=\"index\"\n    >\n      <a href=\"#/\" slot=\"keyword\" slot-scope=\"text\">{{text}}</a>\n      <span slot=\"rang\" slot-scope=\"text\">{{text}} %<a-icon type=\"caret-up\" /> </span>\n    </a-table>\n  </div>\n</template>\n\n<script>\nimport MiniArea from '../../../components/chart/MiniArea'\n\nconst searchData = []\nfor (let i = 0; i < 50; i++) {\n  searchData.push({\n    index: i + 1,\n    keyword: '关键词-' + i,\n    count: Math.floor(Math.random() * 1000),\n    range: Math.floor(Math.random() * 100),\n    status: Math.floor((Math.random() * 10) % 2)\n  })\n}\n\nconst columns = [\n  {\n    dataIndex: 'index',\n    key: 'rank'\n  },\n  {\n    dataIndex: 'keyword',\n    key: 'keyword',\n    scopedSlots: {customRender: 'keyword'}\n  },\n  {\n    dataIndex: 'count',\n    key: 'users',\n    sorter: (a, b) => a.count - b.count\n  },\n  {\n    title: '周涨幅',\n    dataIndex: 'range',\n    key: 'range',\n    scopedSlots: {customRender: 'rang'}\n  }\n]\n\nexport default {\n  name: 'HotSearch',\n  components: {MiniArea},\n  i18n: require('./i18n-search'),\n  data () {\n    return {\n      searchData,\n      columns\n    }\n  },\n  computed: {\n    tableColumns() {\n      let columns = this.columns\n      return columns.map(item => {\n       item.title = this.$t(item.key)\n        return item\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n  .num-info{\n    .title{\n      color: @text-color-second;\n      font-size: 14px;\n      height: 22px;\n      line-height: 22px;\n      overflow: hidden;\n      text-overflow: ellipsis;\n      word-break: break-all;\n      white-space: nowrap;\n    }\n    .value{\n      .total{\n        color: @title-color;\n        display: inline-block;\n        line-height: 32px;\n        height: 32px;\n        font-size: 24px;\n        margin-right: 32px;\n      }\n      .subtotal{\n        color: @text-color-second;\n        font-size: 16px;\n        vertical-align: top;\n        margin-right: 0;\n        i{\n          font-size: 12px;\n          color: red;\n          transform: scale(.82);\n          margin-left: 4px;\n        }\n      }\n    }\n  }\n</style>\n"]}]}