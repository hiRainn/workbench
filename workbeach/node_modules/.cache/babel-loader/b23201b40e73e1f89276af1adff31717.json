I+B,KAAK,CAAC/B,KAAD,CAAL,GAAeA,KAAf,GAAuBgC,MAAM,CAAChC,KAAD,CAAN,CAAciC,IAAd,CAAmB,GAAnB,CAA3B;AAAA,GAFQ,CAApB,CAVyC,CAazC;;AACAP,EAAAA,aAAa,sBAAO,IAAIQ,GAAJ,CAAQR,aAAR,CAAP,CAAb,CAdyC,CAezC;;AACA,MAAIS,SAAS,GAAG,CAACH,MAAM,CAAC3B,OAAD,CAAN,CAAgB4B,IAAhB,CAAqB,GAArB,CAAD,CAAhB;AACA,MAAIG,gBAAgB,GAAGhC,mBAAmB,CAACN,IAAD,CAA1C;AACA,SAAO;AAACO,IAAAA,OAAO,EAAPA,OAAD;AAAUiB,IAAAA,UAAU,EAAVA,UAAV;AAAsBC,IAAAA,SAAS,EAATA,SAAtB;AAAiCL,IAAAA,UAAU,EAAVA,UAAjC;AAA6CQ,IAAAA,aAAa,EAAbA,aAA7C;AAA4DS,IAAAA,SAAS,EAATA,SAA5D;AAAuEC,IAAAA,gBAAgB,EAAhBA;AAAvE,GAAP;AACD;;AAED,SAASJ,MAAT,CAAgBhC,KAAhB,EAAuB;AACrB,MAAI+B,KAAK,CAAC/B,KAAD,CAAT,EAAkB;AAChB,WAAOV,SAAS,CAAC0C,MAAV,CAAiBhC,KAAjB,CAAP;AACD;;AACD,MAAIqC,QAAQ,GAAG,EAAf;;AACA,MAAIC,KAAK,CAACtC,KAAD,CAAT,EAAkB;AAChBqC,IAAAA,QAAQ,GAAGrC,KAAK,CAACe,KAAN,CAAY,CAAZ,EAAef,KAAK,CAACuC,MAArB,CAAX;AACD,GAFD,MAEO,IAAIC,MAAM,CAACxC,KAAD,CAAV,EAAmB;AACxBqC,IAAAA,QAAQ,GAAGrC,KAAK,CAACe,KAAN,CAAY,CAAZ,EAAef,KAAK,CAACyC,WAAN,CAAkB,GAAlB,CAAf,CAAX;AACD;;AACD,MAAIC,GAAG,GAAGL,QAAQ,CAACM,KAAT,CAAe,GAAf,CAAV;AACA,MAAMC,CAAC,GAAGC,QAAQ,CAACH,GAAG,CAAC,CAAD,CAAJ,CAAlB;AACA,MAAMI,CAAC,GAAGD,QAAQ,CAACH,GAAG,CAAC,CAAD,CAAJ,CAAlB;AACA,MAAMK,CAAC,GAAGF,QAAQ,CAACH,GAAG,CAAC,CAAD,CAAJ,CAAlB;AACA,SAAO,CAACE,CAAD,EAAIE,CAAJ,EAAOC,CAAP,CAAP;AACD;;AAED,SAAShB,KAAT,CAAe/B,KAAf,EAAsB;AACpB,SAAOA,KAAK,CAACuC,MAAN,IAAgB,CAAhB,IAAqBvC,KAAK,CAAC,CAAD,CAAL,IAAY,GAAxC;AACD;;AAED,SAASsC,KAAT,CAAetC,KAAf,EAAsB;AACpB,SAAOA,KAAK,CAACuC,MAAN,IAAgB,EAAhB,IAAsBvC,KAAK,CAACe,KAAN,CAAY,CAAZ,EAAe,CAAf,KAAqB,KAAlD;AACD;;AAED,SAASyB,MAAT,CAAgBxC,KAAhB,EAAuB;AACrB,SAAOA,KAAK,CAACuC,MAAN,IAAgB,EAAhB,IAAsBvC,KAAK,CAACe,KAAN,CAAY,CAAZ,EAAe,CAAf,KAAqB,MAAlD;AACD;;AAEDiC,MAAM,CAACC,OAAP,GAAiB;AACflB,EAAAA,KAAK,EAALA,KADe;AAEfO,EAAAA,KAAK,EAALA,KAFe;AAGfE,EAAAA,MAAM,EAANA,MAHe;AAIfR,EAAAA,MAAM,EAANA,MAJe;AAKfjC,EAAAA,aAAa,EAAbA,aALe;AAMfiB,EAAAA,aAAa,EAAbA,aANe;AAOfK,EAAAA,oBAAoB,EAApBA,oBAPe;AAQfjB,EAAAA,mBAAmB,EAAnBA;AARe,CAAjB","sourcesContent":["const varyColor = require('webpack-theme-color-replacer/client/varyColor')\nconst {generate} =  require('@ant-design/colors')\nconst {ADMIN, ANTD} = require('../config/default')\nconst Config = require('../config')\n\nconst themeMode = ADMIN.theme.mode\n\n// 获取 ant design 色系\nfunction getAntdColors(color, mode) {\n  let options = mode && (mode == themeMode.NIGHT) ? {theme: 'dark'} : undefined\n  return generate(color, options)\n}\n\n// 获取功能性颜色\nfunction getFunctionalColors(mode) {\n  let options = mode && (mode == themeMode.NIGHT) ? {theme: 'dark'} : undefined\n  let {success, warning, error} = ANTD.primary\n  const  {success: s1, warning: w1, error: e1} = Config.theme\n  success = success && s1\n  warning = success && w1\n  error = success && e1\n  const successColors = generate(success, options)\n  const warningColors = generate(warning, options)\n  const errorColors = generate(error, options)\n  return {\n    success: [...successColors.slice(0, 3), successColors[5]],\n    warning: [...warningColors.slice(0, 3), warningColors[5]],\n    error: [...errorColors.slice(0, 3), errorColors[5]]\n  }\n}\n\n// 获取菜单色系\nfunction getMenuColors(color, mode) {\n  if (mode == themeMode.NIGHT) {\n    return ANTD.primary.night.menuColors\n  } else if (color == ANTD.primary.color) {\n    return ANTD.primary.dark.menuColors\n  } else {\n    return [varyColor.darken(color, 0.93), varyColor.darken(color, 0.83), varyColor.darken(color, 0.73)]\n  }\n}\n\n// 获取主题模式切换色系\nfunction getThemeToggleColors(color, mode) {\n  //主色系\n  const mainColors = getAntdColors(color, mode)\n  const primary = mainColors[5]\n  //辅助色系，因为 antd 目前没针对夜间模式设计，所以增加辅助色系以保证夜间模式的正常切换\n  const subColors = getAntdColors(primary, themeMode.LIGHT)\n  //菜单色系\n  const menuColors = getMenuColors(color, mode)\n  //内容色系（包含背景色、文字颜色等）\n  const themeCfg = ANTD.theme[mode]\n  let contentColors = Object.keys(themeCfg)\n    .map(key => themeCfg[key])\n    .map(color => isHex(color) ? color : toNum3(color).join(','))\n  // 内容色去重\n  contentColors = [...new Set(contentColors)]\n  // rgb 格式的主题色\n  let rgbColors = [toNum3(primary).join(',')]\n  let functionalColors = getFunctionalColors(mode)\n  return {primary, mainColors, subColors, menuColors, contentColors, rgbColors, functionalColors}\n}\n\nfunction toNum3(color) {\n  if (isHex(color)) {\n    return varyColor.toNum3(color)\n  }\n  let colorStr = ''\n  if (isRgb(color)) {\n    colorStr = color.slice(5, color.length)\n  } else if (isRgba(color)) {\n    colorStr = color.slice(6, color.lastIndexOf(','))\n  }\n  let rgb = colorStr.split(',')\n  const r = parseInt(rgb[0])\n  const g = parseInt(rgb[1])\n  const b = parseInt(rgb[2])\n  return [r, g, b]\n}\n\nfunction isHex(color) {\n  return color.length >= 4 && color[0] == '#'\n}\n\nfunction isRgb(color) {\n  return color.length >= 10 && color.slice(0, 3) == 'rgb'\n}\n\nfunction isRgba(color) {\n  return color.length >= 13 && color.slice(0, 4) == 'rgba'\n}\n\nmodule.exports = {\n  isHex,\n  isRgb,\n  isRgba,\n  toNum3,\n  getAntdColors,\n  getMenuColors,\n  getThemeToggleColors,\n  getFunctionalColors\n}\n"]}]}